{
  "name": "@skylib/quasar-extension",
  "version": "0.1.56",
  "private": false,
  "description": "Quasar extension",
  "keywords": [
    "extension",
    "quasar"
  ],
  "homepage": "https://github.com/ilyub/quasar-extension",
  "bugs": {
    "url": "https://github.com/ilyub/quasar-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/quasar-extension.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "dist/index.js",
  "module": "es/index.js",
  "scripts": {
    "browserslist": "browserslist",
    "build-doc": "typedoc src",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "serve": "quasar dev",
    "stylelint": "stylelint --fix --max-warnings 0 {src,tests}/**/*.{css,scss}",
    "stylelint-html": "stylelint --config .stylelintrc-html.js --fix --max-warnings 0 {src,tests}/**/*.{html,php,vue}",
    "stylelint-html-no-fix": "stylelint --config .stylelintrc-html.js --max-warnings 0 {src,tests}/**/*.{html,php,vue}",
    "stylelint-no-fix": "stylelint --max-warnings 0 {src,tests}/**/*.{css,scss}",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@mdi/js": "^6.6.96",
    "@quasar/app-webpack": "^3.5.1",
    "@quasar/extras": "^1.13.6",
    "@skylib/config": "^1.12.0",
    "@skylib/eslint-plugin": "^0.1.27",
    "core-js": "^3.22.5",
    "flag-icon-css": "^4.1.7",
    "quasar": "^2.6.6",
    "typeface-roboto-multilang": "^1.0.0",
    "vue-router": "^4.0.15",
    "vue-tsc": "^0.34.13"
  },
  "peerDependencies": {
    "@mdi/js-dynamic": "npm:@mdi/js@^6.6.96",
    "@quasar/quasar-app-extension-testing-unit-jest": "3.0.0-alpha.10",
    "@skylib/facades": "^3.0.0",
    "@skylib/framework": "^1.0.0",
    "@skylib/functions": "^2.3.0",
    "@skylib/lodash-commonjs-es": "^0.1.0",
    "@skylib/typescript-types": "^1.0.0",
    "@vue/test-utils": "^2.0.0-rc.21",
    "maska": "^1.5.0",
    "type-fest": "^2.12.2",
    "vue": "^3.2.33",
    "vuedraggable": "^4.1.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
