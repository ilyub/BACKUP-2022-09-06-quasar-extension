{
  "name": "@skylib/quasar-extension",
  "version": "0.1.58",
  "private": false,
  "description": "Quasar extension",
  "keywords": [
    "extension",
    "quasar"
  ],
  "homepage": "https://github.com/ilyub/quasar-extension",
  "bugs": {
    "url": "https://github.com/ilyub/quasar-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/quasar-extension.git"
  },
  "license": "ISC",
  "author": "Ilia Liubinskii <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "dist/index.js",
  "module": "es/index.js",
  "files": [
    "/configs",
    "/dist",
    "/es",
    "/src/components",
    "/src/facade-implementations",
    "/src/index.ts",
    "/src/plugin.ts",
    "/src/style.scss",
    "/src/variables.scss",
    "/src/test-utils"
  ],
  "scripts": {
    "add-tags": "php node_modules/@skylib/config/src/utils/add-tags.php",
    "audit": "npm audit",
    "browserslist": "browserslist",
    "build": "echo Skip build",
    "build-doc": "cf ./docs & typedoc src & cpy docs-static/* docs",
    "build-es": "echo Skip build-es",
    "clean-install": "npm clean-install --no-audit --strict-peer-deps",
    "clear-cache": "cf cache node_modules/.cache",
    "commit-msg": "npm run commitlint",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "compile": "echo Skip compile",
    "config-lint": "node node_modules/@skylib/config/src/utils/config-lint.js",
    "delete-tags": "php node_modules/@skylib/config/src/utils/delete-tags.php",
    "full-check": "php node_modules/@skylib/config/src/utils/full-check.php",
    "install-deps": "npm install --no-audit --strict-peer-deps",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {__mocks__,configs,fixtures,src,tests}/**/*.{js,ts,vue}",
    "lint-fast": "npm run lint --config .eslintrc.fast.js",
    "lint-no-fix": "set TIMING=1 & npm run lint --fix",
    "package-json-lint": "npmPkgJsonLint .",
    "php-cs-fixer": "php-cs-fixer fix --verbose",
    "post-commit": "php node_modules/@skylib/config/src/utils/post-commit.php",
    "pre-commit": "php node_modules/@skylib/config/src/utils/pre-commit.php",
    "prepare": "husky install node_modules/@skylib/config/src/git-hooks",
    "publish": "publish-if-not-exists",
    "regenerate-lock-file": "npm install --no-audit --package-lock-only --strict-peer-deps",
    "serve": "quasar dev",
    "sonar": "sonar-scanner",
    "stylelint": "stylelint --fix --max-warnings 0 src/**/*.{css,scss}",
    "stylelint-html": "stylelint --config .stylelintrc-html.js --fix --max-warnings 0 src/**/*.{html,php,vue}",
    "stylelint-html-no-fix": "stylelint --config .stylelintrc-html.js --max-warnings 0 src/**/*.{html,php,vue}",
    "stylelint-no-fix": "stylelint --max-warnings 0 src/**/*.{css,scss}",
    "test": "cf ./lcov-report & jest",
    "test-fast": "cf ./lcov-report & jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update": "npm update --no-audit --strict-peer-deps",
    "update-package-check": "ncu --dep dev,peer,prod",
    "update-package-force": "ncu --dep dev,peer,prod --upgrade",
    "upgrade": "npm run update-package-force & npm run install-deps",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "devDependencies": {
    "@mdi/js": "^6.7.96",
    "@quasar/app-webpack": "^3.5.4",
    "@quasar/extras": "^1.14.1",
    "@skylib/config": "^3.0.1",
    "@skylib/eslint-plugin": "^0.1.31",
    "core-js": "^3.23.2",
    "date-fns": "^2.28.0",
    "flag-icon-css": "^4.1.7",
    "typeface-roboto-multilang": "^1.0.0",
    "vue-router": "^4.0.16",
    "vue-tsc": "^0.38.1"
  },
  "peerDependencies": {
    "@mdi/js-dynamic": "npm:@mdi/js@^6.7.96",
    "@quasar/quasar-app-extension-testing": ">=2",
    "@quasar/quasar-app-extension-testing-unit-jest": ">=3.0.0-alpha.10",
    "@skylib/facades": "^4.5.1",
    "@skylib/framework": "^1.3.1",
    "@skylib/functions": "^2.5.1",
    "@skylib/lodash-commonjs-es": "^0.1.1",
    "@skylib/typescript-types": "^1.0.0",
    "@types/jquery": ">=3",
    "expect": ">=28",
    "jquery": ">=3",
    "maska": ">=1",
    "quasar": ">=2",
    "ts-toolbelt": ">=9",
    "type-fest": ">=2",
    "vue": ">=3",
    "vuedraggable": ">=4"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  },
  "publishConfig": {
    "access": "public"
  },
  "overrides": {
    "@vue/test-utils": "2.0.0-rc.21",
    "package-json": "8.1.0"
  }
}
