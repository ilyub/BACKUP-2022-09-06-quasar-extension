{
  "name": "@skylib/quasar-extension",
  "version": "0.1.56",
  "private": false,
  "description": "Quasar extension",
  "keywords": [
    "extension",
    "quasar"
  ],
  "homepage": "https://github.com/ilyub/quasar-extension",
  "bugs": {
    "url": "https://github.com/ilyub/quasar-extension/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/ilyub/quasar-extension.git"
  },
  "license": "ISC",
  "author": "Ilya Lyubinskiy <admin.phpdev@gmail.com> (https://github.com/ilyub)",
  "publisher": "ilyub",
  "main": "dist/index.js",
  "module": "es/index.js",
  "scripts": {
    "browserslist": "browserslist",
    "build-doc": "typedoc src",
    "commitlint": "commitlint --edit",
    "commitlint-all": "commitlint --config commitlint-all.config.js --from=",
    "lint": "eslint --fix --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-fast": "eslint --config .eslintrc.fast.js --fix --max-warnings 0 --no-error-on-unmatched-pattern *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "lint-no-fix": "eslint --max-warnings 0 --no-error-on-unmatched-pattern --report-unused-disable-directives *.{js,ts} {src,tests,__mocks__}/**/*.{js,ts,vue}",
    "prepare": "cpy node_modules/@skylib/config/src/git-hooks/* .git/hooks",
    "serve": "quasar dev",
    "stylelint": "stylelint --fix --max-warnings 0 {src,tests}/**/*.{css,scss}",
    "stylelint-html": "stylelint --config .stylelintrc-html.js --fix --max-warnings 0 {src,tests}/**/*.{html,php,vue}",
    "stylelint-html-no-fix": "stylelint --config .stylelintrc-html.js --max-warnings 0 {src,tests}/**/*.{html,php,vue}",
    "stylelint-no-fix": "stylelint --max-warnings 0 {src,tests}/**/*.{css,scss}",
    "test": "jest",
    "test-fast": "jest --config=jest.config.fast.js",
    "tsc": "tsc --noEmit",
    "update-package-check": "ncu --dep dev,prod --reject @types/node",
    "update-package-check-peer": "ncu --dep peer --minimal --reject @types/node",
    "update-package-check-skylib": "ncu --dep peer --filter @skylib/*",
    "update-package-force": "ncu --dep dev,prod --reject @types/node --upgrade",
    "update-package-force-peer": "ncu --dep peer --minimal --reject @types/node --upgrade",
    "update-package-force-skylib": "ncu --dep peer --filter @skylib/* --upgrade",
    "vue-tsc": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@quasar/extras": "^1.13.5",
    "core-js": "^3.22.0",
    "quasar": "^2.6.6"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.17.0",
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@getify/eslint-plugin-proper-arrows": "^11.0.3",
    "@jest/test-sequencer": "^27.5.1",
    "@mdi/js": "^6.6.96",
    "@quasar/app-webpack": "^3.5.1",
    "@quasar/quasar-app-extension-testing": "^2.0.3",
    "@quasar/quasar-app-extension-testing-unit-jest": "^3.0.0-alpha.10",
    "@skylib/config": "^1.6.0",
    "@skylib/eslint-plugin": "^0.1.22",
    "@types/eslint": "^8.4.1",
    "@types/jest": "^27.4.1",
    "@types/node": "^15.14.0",
    "@typescript-eslint/eslint-plugin": "^5.20.0",
    "@typescript-eslint/parser": "^5.20.0",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "autoprefixer": "*",
    "cpy-cli": "^4.1.0",
    "css-property-sort-order-smacss": "^2.2.0",
    "eslint": "^8.13.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-plugin-boundaries": "^2.9.0",
    "eslint-plugin-es": "^4.1.0",
    "eslint-plugin-escompat": "^3.1.0",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-etc": "^2.0.2",
    "eslint-plugin-filenames": "^1.3.2",
    "eslint-plugin-github": "^4.3.6",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.4",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsdoc": "^39.2.3",
    "eslint-plugin-json": "^3.1.0",
    "eslint-plugin-lodash": "^7.4.0",
    "eslint-plugin-more": "^1.0.5",
    "eslint-plugin-no-type-assertion": "^1.3.0",
    "eslint-plugin-no-use-extend-native": "^0.5.0",
    "eslint-plugin-only-warn": "^1.0.3",
    "eslint-plugin-optimize-regex": "^1.2.1",
    "eslint-plugin-pii": "^1.0.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-regexp": "^1.6.0",
    "eslint-plugin-security": "^1.5.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "eslint-plugin-sonarjs": "^0.13.0",
    "eslint-plugin-sort-destructure-keys": "^1.4.0",
    "eslint-plugin-sort-export-all": "^1.2.2",
    "eslint-plugin-sort-keys-fix": "^1.1.2",
    "eslint-plugin-tsdoc": "^0.2.16",
    "eslint-plugin-typescript-sort-keys": "^2.1.0",
    "eslint-plugin-unicorn": "^42.0.0",
    "eslint-plugin-vue": "^8.6.0",
    "eslint-plugin-vue-scoped-css": "^2.2.0",
    "eslint-plugin-write-good-comments": "^0.1.4",
    "eslint-plugin-xss": "^0.1.11",
    "flag-icon-css": "^4.1.7",
    "jest": "^27.5.1",
    "jest-environment-jsdom": "^27.5.1",
    "jest-environment-jsdom-fifteen": "^1.0.2",
    "jest-environment-node": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "npm-check-updates": "^12.5.9",
    "postcss-html": "^1.4.1",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.6.2",
    "stylelint": "^14.7.1",
    "stylelint-no-unsupported-browser-features": "^5.0.3",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-jest": "^27.1.4",
    "typedoc": "^0.22.15",
    "typedoc-plugin-markdown": "^3.12.0",
    "typeface-roboto-multilang": "^1.0.0",
    "vue-eslint-parser": "^8.3.0",
    "vue-router": "^4.0.14",
    "vue-tsc": "^0.34.7"
  },
  "peerDependencies": {
    "@mdi/js-dynamic": "npm:@mdi/js@^6.6.96",
    "@quasar/quasar-app-extension-testing-unit-jest": "*",
    "@skylib/facades": "^2.0.0",
    "@skylib/framework": "^0.1.31",
    "@skylib/functions": "^1.3.3",
    "@skylib/lodash-commonjs-es": "^0.1.0",
    "@skylib/typescript-types": "^1.0.0",
    "@types/lodash": "^4.14.178",
    "@types/lodash-es": "^4.17.5",
    "@vue/test-utils": "^2.0.0-rc.18",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "maska": "^1.5.0",
    "type-fest": "^2.12.2",
    "vue": "^3.2.26",
    "vuedraggable": "^4.1.0"
  },
  "engines": {
    "node": ">=14.0.0",
    "npm": ">=7.0.0"
  }
}
